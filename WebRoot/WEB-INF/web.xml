<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>ListServlet</servlet-name>
    <servlet-class>com.wangzhe.servlet.ListServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteOneServlet</servlet-name>
    <servlet-class>com.wangzhe.servlet.DeleteOneServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteBatchServlet</servlet-name>
    <servlet-class>com.wangzhe.servlet.DeleteBatchServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ListToAddServlet</servlet-name>
    <servlet-class>com.wangzhe.servlet.ListToAddServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddServlet</servlet-name>
    <servlet-class>com.wangzhe.servlet.AddServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>InitTalkServlet</servlet-name>
    <servlet-class>com.wangzhe.servlet.InitTalkServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AutoReplyServlet</servlet-name>
    <servlet-class>com.wangzhe.servlet.AutoReplyServlet</servlet-class>
  </servlet>







  <servlet-mapping>
    <servlet-name>ListServlet</servlet-name>
    <url-pattern>/servlet/ListServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteOneServlet</servlet-name>
    <url-pattern>/servlet/DeleteOneServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteBatchServlet</servlet-name>
    <url-pattern>/servlet/DeleteBatchServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ListToAddServlet</servlet-name>
    <url-pattern>/servlet/ListToAddServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddServlet</servlet-name>
    <url-pattern>/servlet/AddServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>InitTalkServlet</servlet-name>
    <url-pattern>/servlet/InitTalkServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AutoReplyServlet</servlet-name>
    <url-pattern>/servlet/AutoReplyServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
